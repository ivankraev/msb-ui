{
  "name": "msp-hub-front",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.4.0",
    "react-router-dom": "^6.4.0",
    "react-table": "^7.8.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "build": "webpack --config=configs/webpack/prod.js",
    "start": "webpack serve --config=configs/webpack/dev.js",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "yarn run build",
    "test": "jest --watchAll --config=jest.config.js",
    "lint": "eslint . --fix --ext=ts,tsx",
    "lint:styles": "stylelint **.scss --fix",
    "format": "prettier --write ./src",
    "commit": "cz",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.12",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.5",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-jest": "^29.0.1",
    "typescript": "*",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn run lint"
    ],
    "*.{ts,tsx,js,json}": [
      "yarn run format"
    ],
    "*.scss": "yarn run lint:styles"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
